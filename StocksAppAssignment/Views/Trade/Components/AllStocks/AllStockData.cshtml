@model List<USExchange>
<script type="text/javascript">
    const model = @Html.Raw(Json.Serialize(Model))
</script>
<script src="~/Scripts/AllStocks.js"></script>
<button id="load-stocks-button"
        type="button"
        class="button button-blue-back margin-bottom"
        onclick="loadStocks()">
    Load Stocks
</button>
<table id="stocks-table"
       style="display: none"
       class="table w-100">
    <thead>
        <tr>
            <th style="width: 5%">Currency</th>
            <th style="width: 55%">Description</th>
            <th style="width: 15%">Display Symbol</th>
            <th style="width: 15%">Symbol</th>
            <th style="width: 15%">Type</th>
        </tr>
    </thead>
    <tbody id="stock-list">
    </tbody>
</table>
<button id="load-more-button"
        type="button"
        class="button button-green-back margin-top"
        style="display: none"
        onclick="loadNextItems()"
>Load More...</button>